<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER 
		AUTHOR="cimt AG" 
		COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="false" 
		SCHEMA_AUTO_PROPAGATE="false"
		PLATEFORM="ALL" 
		RELEASE_DATE="20160817"
		SERIAL="1" 
		STARTABLE="false" 
		STATUS="PROD" 
		VERSION="1.0">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
	    <FAMILY>Processing/Fields</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0" />
	</CONNECTORS>
	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="1">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER 
			FIELD="CLOSED_LIST" 
			NAME="HASH_TYPE" 
			NUM_ROW="10" 
			REQUIRED="true"
		>
	    	<ITEMS DEFAULT="SHA1">
	    		<ITEM NAME="MD5" VALUE="MD5" />
	    		<ITEM NAME="SHA1" VALUE="SHA1" />
	    		<ITEM NAME="SHA256" VALUE="SHA256" />
	    	</ITEMS>
		</PARAMETER>

		<PARAMETER 
			NAME="OUTPUT_COLUMN" 
			FIELD="COLUMN_LIST"
			NUM_ROW="20"
		>
		</PARAMETER>

		<PARAMETER 
			NAME="COLUMN_CONFIG" 
			FIELD="TABLE" 
			NUM_ROW="30" 
			NB_LINES="10" >
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="USE" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER 
			NAME="CASE_INSENSITIVE" 
			FIELD="CHECK"
			NUM_ROW="40"
		>
		</PARAMETER>

		<PARAMETER 
			NAME="ALLOW_NO_USED_FIELDS" 
			FIELD="CHECK"
			NUM_ROW="40"
		>
		</PARAMETER>

	</PARAMETERS>
	<ADVANCED_PARAMETERS />
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt-talendcomp-checksum" MODULE="cimt-talendcomp-checksum-1.1.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="FLOW" NAME="HASH" TYPE="id_String" />
	</RETURNS>
</COMPONENT>
